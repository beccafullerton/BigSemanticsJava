<meta_metadata_repository name="dream_house" package="ecologylab.semantics.generated.library.products">

  <meta_metadata name="product" extends="document" parser="xpath">
  	<scalar name="price" scalar_type="String" />
    <scalar name="image_url" scalar_type="ParsedURL" hide="true" />
    <scalar name="overall_rating" scalar_type="String" />
    <collection name="reviews" child_type="product_review">
      <scalar name="rating" scalar_type="String" />
      <scalar name="content" scalar_type="String" />
    </collection>
  </meta_metadata>

</meta_metadata_repository>