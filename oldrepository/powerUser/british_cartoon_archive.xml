<meta_metadata_repository>
	<meta_metadata name="british_cartoon_archive"
		comment="This MMD was initially generated by the browser authoring tool"
		layer="0.0" package="ecologylab.semantics.generated.library.british_cartoon_archive"
		extends="document" parser="xpath" visibility="GLOBAL">
		<scalar name="artist" comment="Name of the artist, and period he lived"
			xpath="//*[@id=&#39;detailArtist&#39;]" layer="0.0" scalar_type="String">
			<filter regex="Artist:" replace=""></filter>
		</scalar>
		<scalar name="published_in"
			comment="Name of the publication in which the cartoon appeared"
			xpath="//*[@id='detailPublish']" layer="0.0" scalar_type="String">
			<filter regex="(?&lt;=Published:)([^,]+)" ></filter>
			</scalar>
		<scalar name="publication_date"
			comment="Date of the publication"
			xpath="//*[@id='detailPublish']" layer="0.0" scalar_type="Date">
			<filter regex="Published:[^,]+," replace=""></filter>
			</scalar>
		<scalar name="publication_format" comment="Format of the publication"
			xpath="//*[@id=&#39;detailFormat&#39;]" layer="0.0" scalar_type="String">
			<filter regex="Format:" replace=""></filter>
			</scalar>
		
		<scalar name="caption" comment="Cartoon Caption"
			xpath="//*[@class=&#39;captionClass&#39;]" layer="0.0" scalar_type="String">
			<filter regex="Caption" replace=""></filter>
			</scalar>
			
		<scalar name="reference_number" 
			xpath="//*[@id=&#39;refNumber&#39;]" layer="0.0" scalar_type="String">
			<filter regex="Reference number:" replace=""></filter>
			</scalar>
		<scalar name="img_location" comment="url of the image"
			xpath="//*[@id=&#39;mediumImage&#39;]//img/@src" layer="0.0"
			scalar_type="String"></scalar>
		<selector url_path_tree="http://www.cartoons.ac.uk/record/"></selector>
	</meta_metadata>
</meta_metadata_repository>