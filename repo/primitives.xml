<meta_metadata_repository name="primitives" default_user_agent_name="chrome_2"
 package="ecologylab.semantics.generated.library"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" >
		
	<!--+++++++++++++++++++++++++++++++++ Builtin Primitive Base Types  +++++++++++++++++++++++++++++++++-->
	<meta_metadata name="metadata" dont_generate_class="true" comment="The Metadata Class">
		<scalar name="meta_metadata_name" scalar_type="String" hide="true" ignore_in_term_vector="true" comment="Stores the name of the meta-metadata, and is used on restoring from XML." />
	</meta_metadata>
	
	<meta_metadata name="document" dont_generate_class="true" extends="metadata" comment="The Document Class">
		<scalar name="title" scalar_type="String" style="h1" layer="10" navigates_to="location" comment="The Title of the Document" />
		<scalar name="description" scalar_type="String" layer="9" />
		<scalar name="location" scalar_type="ParsedURL" layer="8" always_show="true" comment="The document's actual location." />
		<scalar name="generation" scalar_type="Int" layer="7" />
		<scalar name="page_structure" scalar_type="String" layer="6" comment="For debugging. Type of the structure recognized by information extraction." />
		<scalar name="query" scalar_type="String" layer="5" comment="The search query" />
	</meta_metadata>
	
	<meta_metadata name="entity" dont_generate_class="true" extends="metadata" comment="Reference to a nested document.">
		<scalar name="location" scalar_type="ParsedURL" hide="true" comment="The document's actual location." />
		<scalar name="gist" scalar_type="String" navigates_to="location" comment="Initial text that describes the document before its downloaded." />
	</meta_metadata>
	
	<meta_metadata name="pdf" extends="document" comment="The PDF class." parser="pdf">
		<scalar name="author" scalar_type="String" comment="The author of the pdf document" />
		<scalar name="summary" scalar_type="String" comment="Summary of the pdf document" />
		<scalar name="keywords" scalar_type="String" comment="Key Words of the document" />
		<scalar name="subject" scalar_type="String" comment="Subject of the document" />
		<scalar name="trapped" scalar_type="String" />
		<scalar name="modified" scalar_type="String" />
		<scalar name="contents" scalar_type="String" comment="Contents of the document" />
		<scalar name="creationdate" scalar_type="String" comment="Creation date of the document" />
		<mime_type>application/pdf</mime_type>
		<mime_type>application/x-pdf</mime_type>
		<mime_type>image/pdf</mime_type>
		<suffix>pdf</suffix>
	</meta_metadata>
	
	<meta_metadata name="media" dont_generate_class="true" extends="metadata" comment="Extend this for Image, sounds, video, etc.">
		<scalar name="context" scalar_type="String" layer="10" />
		<scalar name="meta_metadata_name" scalar_type="String" hide="true" ignore_in_term_vector="true" comment="Stores the name of the meta-metadata, and is used on restoring from XML." />
	</meta_metadata>
	
	<meta_metadata name="image" dont_generate_class="true" extends="media" comment="The Image Base Class">
		<scalar name="caption" scalar_type="String" style="h1" layer="11" comment="The caption of the image." />
		<scalar name="location" scalar_type="ParsedURL" layer="9" comment="Location of the image." />
		<scalar name="local_location" scalar_type="String" hide="true" comment="Relative location of local copy of image." />
		<mime_type>image/jpeg</mime_type>
		<mime_type>image/png</mime_type>
		<mime_type>image/gif</mime_type>
		<mime_type>image/bmp</mime_type>
		<suffix>jpg</suffix>
		<suffix>jpeg</suffix>
		<suffix>gif</suffix>
		<suffix>png</suffix>
		<suffix>bmp</suffix>
	</meta_metadata>
	
	<meta_metadata name="debug_metadata" dont_generate_class="true">
		<scalar name="new_term_vector" scalar_type="StringBuilder" ignore_in_term_vector="true" />
	</meta_metadata>
	
</meta_metadata_repository>
