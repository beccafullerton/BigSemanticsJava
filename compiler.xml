<?xml version="1.0" encoding="UTF-8"?>
<!-- Run MetaMetadataCompiler. Launch configuration has refresh setting for GeneratedSemantics so that there is no need to manually refresh -->
<project basedir="." default="compile" name="ecologylabSemantics">
    <property name="ecologylabFundamental.location" value="../ecologylabFundamental"/>
    <property name="jtidy.location" value="../jtidy"/>
    <property name="ecologylabInteractive.location" value="../ecologylabInteractive"/>
    <property name="ecologylabSemantics.location" value="../ecologylabSemantics"/>
    <path id="ecologylabFundamental.classpath">
        <pathelement location="${ecologylabFundamental.location}/."/>
    </path>
    <path id="jtidy.classpath">
        <pathelement location="${jtidy.location}/."/>
    </path>
    <path id="ecologylabInteractive.classpath">
        <pathelement location="${ecologylabInteractive.location}/."/>
        <path refid="ecologylabFundamental.classpath"/>
        <path refid="jtidy.classpath"/>
    </path>
    
    <target name="compile">
    	<java classname="ecologylab.semantics.tools.MetaMetadataCompiler">
    		<classpath>
				<path id="ecologylabSemantics.classpath">
			        <pathelement location="."/>
			        <path refid="ecologylabFundamental.classpath"/>
			        <path refid="ecologylabInteractive.classpath"/>
			        <path refid="jtidy.classpath"/>
			    </path>
    		</classpath>
    	</java>
	</target>
</project>
